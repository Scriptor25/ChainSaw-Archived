thing: person {
    name: string,
    age: number,
    address: string,
    plz: number,
    e_mail: string
}

$person [name: string, age: number, address: string, plz: number, e_mail: string] {
    my.name = name;
    my.age = age;
    my.address = address;
    my.plz = plz;
    my.e_mail = e_mail;
}

say [msg: string] -> person {
    out("%s says \"%s\"%n", my.name, msg);
}

fib: number [n: number] {
    ret n == 0 ? 0 : n == 1 ? 1 : fib(n - 1) + fib(n - 2);
}

main: number {

    person p = person("Felix Schreiber", 17, "Somewhere over the rainbow", 12345, "f.schreiber.2000@web.de");
    p.say("Hi!");

    for (number i = 0; i <= 10; i++)
        out("fib(%s) = %s%n", i, fib(i));

    ret 0;
}
